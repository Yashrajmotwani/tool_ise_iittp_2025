{
  "name": "complexity-analyzer",
  "displayName": "Complexity Analyzer",
  "description": "Analyzes time and space complexity of C++ code using AST with visual feedback",
  "version": "1.0.0",
  "publisher": "your-publisher-id",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [],
  "contributes": {
    "languages": [
      {
        "id": "cpp",
        "aliases": [
          "C++",
          "cpp"
        ],
        "extensions": [
          ".cpp",
          ".cc",
          ".cxx",
          ".h",
          ".hpp"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "commands": [
      {
        "command": "complexity-analyzer.showComplexity",
        "title": "Show Time Complexity"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "tree-sitter": "^0.21.1",
    "tree-sitter-cpp": "^0.23.4"
  }
}