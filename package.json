{
  "name": "code-review-helper",
  "displayName": "code-review-helper",
  "description": "A code review checklist for refactoring suggestions, test case creation, summary and code complexity",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages",
    "Linters",
    "Other"
  ],
  "activationEvents": [
    "onCommand:code-review-helper.detectFunctions"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-review-helper.detectFunctions",
        "title": "Code Review Helper: Detect Functions"
      },
      {
        "command": "heatmap.toggleHeatmap",
        "title": "Toggle Heatmap"
      },
      {
        "command": "heatmap.openWebView",
        "title": "Open Heatmap WebView"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "vscode-test",
    "start": "npx vscode-debug"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.99.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "lizard": "^1.0.0"
  },
  "extensionDependencies": [
    "ms-python.python"
  ]
}
